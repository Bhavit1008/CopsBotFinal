<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newchatui">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <permission
        android:name="com.example.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.permission.MAPS_RECEIVE" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".chat.awarness.MainAwarness"></activity>
        <activity android:name=".features.statusTracking.StatusTrackingActivity" />
        <activity android:name=".chat.awarness.OtherActiviy" />
        <activity android:name=".chat.awarness.RoadAccident" />
        <activity android:name=".chat.awarness.ChildrenSafety" />
        <activity android:name=".chat.awarness.WomenSafety" />
        <activity android:name=".chat.awarness.CyberCrimeSafetyMeasures" />
        <activity android:name=".chat.MainActivity" />
        <activity android:name=".chat.awarness.CyberCrimeCategory" />
        <activity android:name=".features.awarnessPages.TwitterActivity" />
        <activity android:name=".features.awarnessPages.RoadSafetyActivity" />
        <activity android:name=".features.maps.ComplaintActivity" />
        <activity android:name=".features.alerts.AlertsActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".features.voiceNote.VoiceNoteActivity" />
        <activity android:name=".features.ReportIncidentActivity" />
        <activity android:name=".ui.DashBoardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.RegistrationActivity" />
        <activity android:name=".features.maps.MapsComplaint" />
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".chat.IntroActivity" />
        <activity android:name=".chat.DateTimePickerActivity" />
        <activity android:name=".chat.ComplaintDetailsActivity" />
        <activity android:name=".chat.DescriptionActivity" />
        <activity android:name=".chat.DateLocActivity" />
        <activity android:name=".chat.CrimeReportActivity" />
        <activity android:name=".chat.GenderActivity" />
        <activity android:name=".chat.AgeActivity" />
    </application>

</manifest>